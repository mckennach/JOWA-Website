{
  "private": true,
  "scripts": {
    "dev": "graphql-codegen --config codegen.ts && node ./add-ts-nocheck.js && next dev",
    "clean": "rm -rf .next && rm -rf node_modules && npm install",
    "build": "graphql-codegen --config codegen.ts && node ./add-ts-nocheck.js && npm run tw:build && next build",
    "start": "npm run dev",
    "start:dev": "NODE_ENV=development next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts && node ./add-ts-nocheck.js",
    "codegen:watch": "graphql-codegen --config --watch codegen.ts && node ./add-ts-nocheck.js",
    "tw:watch": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/globals.min.css --watch --minify",
    "tw:build": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/globals.min.css --minify",
    "format": "prettier --write ."
  },
  "dependencies": {
